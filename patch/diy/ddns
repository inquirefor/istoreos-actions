
config ddns 'global'
	option ddns_dateformat '%F %R'
	option ddns_loglines '250'
	option ddns_rundir '/var/run/ddns'
	option ddns_logdir '/var/log/ddns'

config service 'myddns_ipv6'
	option use_ipv6 '1'
	option lookup_host 'ipv6.dynv6.com'
	option service_name 'dynv6.com'
	option enabled '1'
	option domain 'jac88.dynv6.net'
	option username 'none'
	option password 'P_NQdyFx_3HrkbNLxyu6yu-tbF8UQC'
	option use_syslog '2'
	option check_interval '5'
	option check_unit 'minutes'
	option force_interval '30'
	option force_unit 'minutes'
	option retry_unit 'seconds'
	option ip_source 'interface'
	option ip_interface 'br-lan'
	option interface 'br-lan'

